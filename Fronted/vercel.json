{
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "cleanUrls": true,
  "redirects": [
    { "source": "/src/assets/(.*)", "destination": "/assets/$1", "permanent": true },
    { "source": "/src/styles.css", "destination": "/styles.css", "permanent": true }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ],
  "trailingSlash": false
}


  